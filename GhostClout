using System;

namespace ConsoleCreatures
{
  class Program
  {
    static void Main()
    {
      //Building ASCII ghost art
      Console.WriteLine(" .-.");    
      Console.WriteLine("(o o)");
      Console.WriteLine("| 0 |");
      Console.WriteLine("|   |");
      Console.WriteLine("'~~~'");
      Console.WriteLine("Clout");
     }
  }
}
